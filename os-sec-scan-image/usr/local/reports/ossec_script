<script>
    var tabs = document.getElementsByClassName("tab_links");
    tabs[0].id = "cve_sum";
    tabs[1].id = "cve_grid";
    document.getElementById("cve_grid").onclick = cve_grid;
    document.getElementById("cve_sum").onclick = cve_sum;

    function cve_grid(evt) {
        var i, tabContent, tabLinks;
        // Get all elements with class="tab_content" and hide them
        tabContent = document.getElementsByClassName("tab_content");
        for (i = 0; i < tabContent.length; i++) {
            tabContent[i].style.display = "none";
        }
        // Get all elements with class="tab_links" and remove the class "active"
        tabLinks = document.getElementsByClassName("tab_links");
        for (i = 0; i < tabLinks.length; i++) {
            tabLinks[i].className = tabLinks[i].className.replace(" active", "");
        }
        // Show the current tab, and add an "active" class to the button that opened the tab
        document.getElementById("vulnDetails").style.display = "inline";
        evt.currentTarget.className += " active";
    }
    function cve_sum(evt) {
        var i, tabContent, tabLinks;
        // Get all elements with class="tab_content" and hide them
        tabContent = document.getElementsByClassName("tab_content");
        for (i = 0; i < tabContent.length; i++) {
         tabContent[i].style.display = "none";
        }
        // Get all elements with class="tab_links" and remove the class "active"
        tabLinks = document.getElementsByClassName("tab_links");
        for (i = 0; i < tabLinks.length; i++) {
         tabLinks[i].className = tabLinks[i].className.replace(" active", "");
        }
        // Show the current tab, and add an "active" class to the button that opened the tab
        document.getElementById("cveSummary").style.display = "inline";
        evt.currentTarget.className += " active";
    }
</script>

<script>
    //Add details trivy table
    var table = document.getElementsByTagName("table")[0];
    var details = document.createElement("details");
    details.innerHTML = "<summary>Show Trivy vulnerability list</summary>\n <table>"+table.innerHTML+"</table>";
    table.replaceWith(details);

    //Delete snyk first table
    var layout = document.getElementsByClassName("layout-container")[1];
    layout.remove();

    //Add details snyk vulns lists
    var vuln_grid = document.getElementById("vulnDetails");
    vuln_grid.innerHTML = "<details><summary>Display vulnerability detailed list</summary>\n"+vuln_grid.innerHTML+"</details>";
    var vuln_summary = document.getElementById("cveSummary");
    vuln_summary.innerHTML = "<details><summary>Display vulnerability grid summary</summary>\n"+vuln_summary.innerHTML+"</details>";

    //Add details dockle table
    var layout = document.getElementsByClassName("json-pretty")[1];
    var details = document.createElement("details");
    var div = document.createElement("div");
    div.classList.add("json-pretty");
    div.classList.add("layout-container");
    div.innerHTML = layout.innerHTML;
    details.innerHTML = "<summary>Show Dockle security best practices</summary>\n"+div.outerHTML;
    layout.replaceWith(details);
</script>
